{{define "content"}}

<div class="sidenav my-1 bg-light shadow  borderRadiusTop borderRadiusBottom">
  <a class="side-nav-link border-end border-danger myTint6BG" href="#" id="personalMenu"><i class="bi-person-bounding-box"></i> Personal</a>
  <a class="side-nav-link" href="#" id="spouseMenu"><i class="bi-person-bounding-box"></i> Spouse</a>
  <a class="side-nav-link" href="#" id="employmentMenu"><i class="bi-person-bounding-box"></i> Employment</a>
